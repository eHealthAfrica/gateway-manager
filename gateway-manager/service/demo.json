{
  "name": "demo-service",
  "host": "http://demo-service:3013",
  "oidc_endpoints": [
    {
      "name": "base",
      "paths": [
        "/{realm}/{name}/",
        "/{realm}/\\d+/{name}/"
      ],
      "strip_path": "false",
      "regex_priority": 1
    }
  ],
  "public_endpoints": [
    {
      "name": "public",
      "paths": [
        "/{public_realm}/{name}/",
        "/~/{name}/public",
        "/-/\\d+/{name}/"
      ],
      "strip_path": "true",
      "regex_priority": 0
    }
  ]
}
